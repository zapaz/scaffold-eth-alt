{
	"name": "@scaffold-eth-alt/sveltekit",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"clean": "rm -rf node_modules build .svelte-kit .turbo",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write --list-different .",
		"vercel": "vercel",
		"fleek:login": "fleek login",
		"deploy:fleek": "pnpm run build && fleek sites deploy"
	},
	"devDependencies": {
		"@fleek-platform/cli": "^2.8.6",
		"@sveltejs/adapter-static": "^3.0.2",
		"@sveltejs/kit": "^2.5.18",
		"@sveltejs/vite-plugin-svelte": "^3.1.1",
		"@types/eslint": "^8.56.10",
		"@typescript-eslint/eslint-plugin": "^7.16.1",
		"@typescript-eslint/parser": "^7.16.1",
		"autoprefixer": "^10.4.19",
		"daisyui": "^4.12.10",
		"eslint": "^9.7.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.42.0",
		"postcss": "^8.4.39",
		"postcss-load-config": "^6.0.1",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.6",
		"prettier-plugin-tailwindcss": "^0.6.5",
		"svelte": "5.0.0-next.184",
		"svelte-check": "^3.8.4",
		"svelte-hero-icons": "^5.2.0",
		"tailwindcss": "^3.4.6",
		"tslib": "^2.6.3",
		"type-fest": "^4.22.0",
		"typescript": "^5.5.3",
		"vercel": "^34.4.0",
		"vite": "^5.3.4",
		"vitest": "^2.0.3"
	},
	"type": "module",
	"dependencies": {
		"@byteatatime/wagmi-svelte": "^0.1.1",
