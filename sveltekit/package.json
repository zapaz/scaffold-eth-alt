{
	"name": "@kredeum-template/sveltekit",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"format": "prettier --write .",
		"check:eslint": "prettier --check . && eslint .",
		"check:svelte": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check": "pnpm run check:eslint && pnpm run check:svelte",
		"build": "vite build",
		"test": "pnpm run test:integration && pnpm run test:unit",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"dev": "vite dev",
		"preview": "vite preview"
	},
	"devDependencies": {
		"@playwright/test": "^1.45.1",
		"@sveltejs/adapter-auto": "^3.2.2",
		"@sveltejs/adapter-static": "^3.0.2",
		"@sveltejs/kit": "^2.5.18",
		"@sveltejs/vite-plugin-svelte": "^3.1.1",
		"@types/eslint": "^8.56.10",
		"eslint": "^9.6.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.41.0",
		"globals": "^15.8.0",
		"prettier": "^3.3.2",
		"prettier-plugin-svelte": "^3.2.5",
		"svelte": "^4.2.18",
		"svelte-check": "^3.8.4",
		"tslib": "^2.6.3",
		"typescript": "^5.5.3",
		"typescript-eslint": "8.0.0-alpha.41",
		"viem": "^2.17.3",
		"vite": "^5.3.3",
		"vitest": "^1.6.0"
	},
	"type": "module",
	"dependencies": {
		"@kredeum-template/contracts": "workspace:^"
	}
}
